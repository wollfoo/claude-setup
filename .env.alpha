# Phase 3 Adaptive Threshold System - Alpha Environment
# Mode: Development/Alpha (in-memory, no persistence)

# Application Settings
NODE_ENV=development
USE_PERSISTENCE=false
LOG_LEVEL=info

# Adaptive Threshold Configuration
BASE_THRESHOLD=0.70
MIN_THRESHOLD=0.60
MAX_THRESHOLD=0.80
ENABLE_ADJUSTMENT=true
CACHE_TTL=300

# Resource Monitoring (Mock values for Alpha)
MEMORY_USAGE=0.55
CPU_USAGE=0.45
DISK_USAGE=0.70

# Alpha Testing Mode
ALPHA_MODE=true
VERBOSE_LOGGING=true
